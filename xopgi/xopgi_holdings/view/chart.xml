<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
    <record id="xopgi_holdings_chart_form" model="ir.ui.view">
      <field name="name">xopgi.holdings.chart.form</field>
      <field name="model">account.chart</field>
      <field name="inherit_id" ref="account.view_account_chart" />
      <field name="arch" type="xml">
        <xpath expr="//label[@for='fiscalyear']" position="before">
          <field name="chart_mode"/>
        </xpath>
      </field>
    </record>
  </data>
</openerp>
